{
    "name": "webfactory/slimdump",
    "description": "slimdump is a little tool to help you creating dumps of large MySQL-databases.",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "webfactory GmbH",
            "email": "info@webfactory.de",
            "homepage": "http://www.webfactory.de",
            "role": "Developer"
        }
    ],
    "bin": [
        "bin/slimdump"
    ],
    "require": {
        "php": ">=5.3.9",
        "doctrine/dbal": "^2.5.2",
        "symfony/console": "^3.0.0 || ^2.8.7",
        "fzaninotto/faker": "^1.6"
    },
    "autoload": {
        "files": [
            "php/util.php"
        ],
        "psr-0": {
            "Webfactory": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "satooshi/php-coveralls": "^0.6.1",
        "kherge/box": "^2.6"
    }
}
